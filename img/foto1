        function loadScript( url, callback ) {
          var script = document.createElement( "script" )
          script.type = "text/javascript";
          if(script.readyState) {  //IE
            script.onreadystatechange = function() {
              if ( script.readyState === "loaded" || script.readyState === "complete" ) {
                script.onreadystatechange = null;
                callback();
              }
            };
          } else {  //Others
            script.onload = function() {
              callback();
            };
          }

          script.src = url;
          document.getElementsByTagName( "head" )[0].appendChild( script );
        }

        function loadComentariosWidget(){
          
          loadScript("/Comentarios/comentarios_js_v3.html?ghi=1549380626-fa52fb5845b1536c2f7def2dc8b027fe&mfo=0&gpi=0&gww=100%&ghw=1&gcss=/css/v1.x/v1.3/secciones/comentarios_v2.css&gjs=1&gll=1&gla=es", function() {

              if(typeof setNumMensajescomentarios !== 'undefined'){
                ESKUP.setOnReceiveNumMensajes("comentarios_noticia_1549380626-fa52fb5845b1536c2f7def2dc8b027fe", setNumMensajescomentarios);
              }
              if(typeof as_omn_f_trackComentarios !== 'undefined'){
                  ESKUP.setOnTrackWidgetNewComment("comentarios_noticia_1549380626-fa52fb5845b1536c2f7def2dc8b027fe", as_omn_f_trackComentarios);
                }
           });
        }

        if(window.addEventListener){
            window.addEventListener("load", loadComentariosWidget);
        }else if(window.attachEvent){
            window.attachEvent("onload", loadComentariosWidget);
        }
      
    </script>
<script type="text/javascript">

function Outbrain(url, template){
    var instance = this;
    this._url = url;
    this._obSup = document.getElementById('outbrain_widget_superior');
    this._obInf = document.getElementById('outbrain_widget_inferior');
    this._obScript = document.getElementById('outbrain_widget_script');
    this._template = template || 'diarioas';
    this._dHeight = Math.max(document.body.scrollHeight, document.body.offsetHeight,
        document.documentElement.clientHeight, document.documentElement.scrollHeight,
        document.documentElement.offsetHeight);
    this._obSupTop = (this._obSup) ? this._obSup.getBoundingClientRect().top - 900 : -1;
    this._obInfTop = (this._obInf) ? this._obInf.getBoundingClientRect().top - 900 : -1;
    //console.log("OUTBRAIN: screen.height = " + this._dHeight);
    //console.log("OUTBRAIN: window.innerHeight = " + window.innerHeight);
    //console.log("OUTBRAIN: obSupTop = " + this._obSupTop);    
    window.outbrainLazy = function(){ __outbrain.lazy(); }
    if(this._obSupTop > 0 && this._dHeight && this._dHeight > window.innerHeight){
        console.log("OUTBRAIN: Create Listener");
        if(document.addEventListener){
            document.addEventListener("scroll", outbrainLazy);
        }else if(document.attachEvent){
            document.attachEvent("onscroll", outbrainLazy);
        }
    }else{
        this.render();
    }
}

Outbrain.prototype.lazy = function(){
    var obSupTop = (this._obSup) ? this._obSup.getBoundingClientRect().top - 900 : -1;
    var obInfTop = (this._obInf) ? this._obInf.getBoundingClientRect().top - 900 : -1;
    //console.log('OUTBRAIN: ' + window.scrollY + ' > ' + obSupTop + ' | ' + this._obSupTop);
    if( (obSupTop != -1 && obSupTop < 0)
        || (obInfTop != -1 && obInfTop < 0)){
        this.render();
        console.log('OUTBRAIN: Remove Listener');
        if(document.removeEventListener){
            document.removeEventListener("scroll", outbrainLazy);
        }else if (document.detachEvent){
            document.detachEvent("onscroll", outbrainLazy);
        }
    }
}

Outbrain.prototype.render = function(){
    console.log("OUTBRAIN: Render");
    this.build(this._obSup, 'AR_40');
    this.build(this._obInf, 'AR_41');
    this.build(this._obInf, 'AR_42');
    if(this._obScript && this._obScript.getAttribute('data-src')){
        console.log('OUTBRAIN: Lazy Render');
        this._obScript.setAttribute('src', this._obScript.getAttribute('data-src'));
        this._obScript.removeAttribute('data-src');
    }else if(typeof OBR !== "undefined"){
        console.log('OUTBRAIN: Function Render Call');
        OBR.extern.researchWidget();
    }
}

Outbrain.prototype.build = function(e,id){
    var d = document.createElement("div");
    d.className = "OUTBRAIN";
    d.setAttribute("data-src", this._url);
    d.setAttribute("data-ob-template", this._template);
    d.setAttribute("data-widget-id", id);
    e.appendChild(d);
}

</script>
<script type="text/javascript">
function loadOutbrainWidget(){
    loadScript("//widgets.outbrain.com/outbrain.js", function(){
        window.__outbrain = new Outbrain("https://as.com/meristation/2019/02/05/betech/1549380316_024009.html", "ES_Meristation");
    });
}
if(window.addEventListener){
    window.addEventListener("load", loadOutbrainWidget);
}else if(window.attachEvent){
    window.attachEvent("onload", loadOutbrainWidget);
}
</script>
</body>
</html>
